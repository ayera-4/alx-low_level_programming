#include "variadic_functions.h"
#include <stdio.h>
#include <stdarg.h>

/**
 * print_numbers - print numbers
 * @separator: input parameter
 * @n: input parameter
 * Return: 0
 */
void print_numbers(const char *separator, const unsigned int n, ...)
{
va_list(arg);
int i;
va_start(arg, n);
for (i = 0; i < n; i++)
{
printf("%i", va_arg(arg, int));
if (separator != NULL && i != (n - 1))
printf("%s", separartor);
}
va_end(arg);
}
  
